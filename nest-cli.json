{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "logging": {
      "type": "library",
      "root": "libs/logging",
      "entryFile": "index",
      "sourceRoot": "libs/logging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logging/tsconfig.lib.json"
      }
    },
    "health": {
      "type": "library",
      "root": "libs/health",
      "entryFile": "index",
      "sourceRoot": "libs/health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health/tsconfig.lib.json"
      }
    },
    "validation": {
      "type": "library",
      "root": "libs/validation",
      "entryFile": "index",
      "sourceRoot": "libs/validation/src",
      "compilerOptions": {
        "tsConfigPath": "libs/validation/tsconfig.lib.json"
      }
    },
    "nest-knex": {
      "type": "library",
      "root": "libs/nest-knex",
      "entryFile": "index",
      "sourceRoot": "libs/nest-knex/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-knex/tsconfig.lib.json"
      }
    },
    "test": {
      "type": "library",
      "root": "libs/test",
      "entryFile": "index",
      "sourceRoot": "libs/test/src",
      "compilerOptions": {
        "tsConfigPath": "libs/test/tsconfig.lib.json"
      }
    }
  }
}